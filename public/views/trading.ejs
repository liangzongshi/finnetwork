											<td id="<%= st+1 + '_type' %>" width="1%" class="with-img"><img src="../assets/coin/xlm.png" class="img-rounded width-30 height-30" /></td>
											<% }else if(item.type == "ETC"){ %>
											<td id="<%= st+1 + '_type' %>" width="1%" class="with-img"><img src="../assets/coin/etc.png" class="img-rounded width-30 height-30" /></td>
											<% }else if(item.type == "ATOM"){ %>
											<td id="<%= st+1 + '_type' %>" width="1%" class="with-img"><img src="../assets/coin/atom.png" class="img-rounded width-30 height-30" /></td>
											<% }else if(item.type == "XTZ"){ %>
											<td id="<%= st+1 + '_type' %>" width="1%" class="with-img"><img src="../assets/coin/xtz.png" class="img-rounded width-30 height-30" /></td>
											<% }else if(item.type == "OMG"){ %>
