						<div class="panel-body">
							<table id="trading_tb" class="table table-striped table-bordered table-td-valign-middle text-center">
								<thead>
									<tr>
										<th width="1%"></th>
										<th class="text-nowrap">Timestamps</th>
										<th class="text-nowrap">Type</th>
										<th class="text-nowrap">Amount</th>
										<th class="text-nowrap">Buy Price</th>
										<th class="text-nowrap">Sell Price</th>
										<th class="text-nowrap">Profit</th>
									</tr>
								</thead>
								<tbody id="hft">
									<% hftOrders.orders.forEach(function(item, st){ %>
										<tr class="odd gradeX" id="">
											<td id="<%= st+1 + '_number' %>" width="1%" class="f-s-600 text-inverse"><%= st+1 %></td>
											<td id="<%= st+1 + '_type' %>" id="<%= st+1 + '_timestamp' %>"><%= item.timestamp %></td>
											<% if(item.type == "BTC"){ %>
											<td id="<%= st+1 + '_type' %>" width="1%" class="with-img"><img src="../assets/coin/btc.png" class="img-rounded width-30 height-30" /></td>
											<% }else if(item.type == "ETH"){ %>
											<td id="<%= st+1 + '_type' %>" width="1%" class="with-img"><img src="../assets/coin/eth.png" class="img-rounded width-30 height-30" /></td>
